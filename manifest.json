{
    "manifest_version": 3,
    "name": "__MSG_extName__",
    "version": "2.0",
    "description": "Mengembalikan fungsi scroll ke atas saat logo YouTube di klik.",
    "default_locale": "en",
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "jquery.min.js",
                "content.js"
            ]
        }
    ]
}